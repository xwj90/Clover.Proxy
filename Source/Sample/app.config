<?xml version="1.0"?>

<configuration>
  <configSections>
    <section name="CloverProxy" type="Clover.Proxy.ProxyConfigSection,Clover.Proxy"/>
  </configSections>
  <CloverProxy>
    <add key="DisableAutoProxy" value ="false" />
    <add key="EnableDebug" value ="true" />
    <add key="ProxyType" value ="Default" />
    <add key="DllCachedPath" value ="xxxxx" />
  </CloverProxy>
  <appSettings>
    <add key="Clover.Proxy.DisableAutoProxy" value="false" />
    <add key="Clover.Proxy.EnableDebug" value="true" />
    <add key="Clover.Proxy.ProxyType" value="Default" />
    <add key="Clover.Proxy.DllCachedPath" value="" />
    
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
</configuration>